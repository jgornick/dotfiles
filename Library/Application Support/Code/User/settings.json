// Place your settings in this file to overwrite the default settings
{
    "[dockercompose]": {
        "editor.autoIndent": "advanced",
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.insertSpaces": true,
        "editor.tabSize": 2
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "application.shellEnvironmentResolutionTimeout": 20,
    "breadcrumbs.enabled": true,
    "chat.agent.maxRequests": 50,
    "chat.editor.fontFamily": "SauceCodePro Nerd Font Mono",
    "chat.editor.fontSize": 16,
    "chat.fontSize": 16,
    "css.validate": true,
    "debug.console.fontFamily": "SauceCodePro Nerd Font Mono",
    "debug.console.fontSize": 16,
    "debug.javascript.codelens.npmScripts": "never",
    "diffEditor.renderSideBySide": true,
    "editor.accessibilitySupport": "off",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "always",
        "source.fixAll.tslint": "never"
    },
    "editor.codeLensFontFamily": "SauceCodePro Nerd Font Mono",
    "editor.codeLensFontSize": 16,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.detectIndentation": true,
    "editor.dragAndDrop": false,
    "editor.emptySelectionClipboard": false,
    "editor.fontFamily": "SauceCodePro Nerd Font Mono",
    "editor.fontSize": 16,
    "editor.guides.indentation": true,
    "editor.hover.delay": 1000,
    "editor.inlayHints.fontFamily": "SauceCodePro Nerd Font Mono",
    "editor.inlayHints.fontSize": 16,
    "editor.inlineSuggest.fontFamily": "SauceCodePro Nerd Font Mono",
    "editor.largeFileOptimizations": false,
    "editor.linkedEditing": true,
    "editor.matchBrackets": "always",
    "editor.minimap.enabled": true,
    "editor.minimap.showSlider": "always",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.parameterHints.cycle": true,
    "editor.renderWhitespace": "all",
    "editor.roundedSelection": false,
    "editor.rulers": [
        80,
        100,
        120
    ],
    "editor.scrollBeyondLastLine": true,
    "editor.showFoldingControls": "always",
    "editor.smoothScrolling": true,
    "editor.snippetSuggestions": "none",
    "editor.stickyScroll.enabled": false,
    "editor.stickyScroll.scrollWithEditor": false,
    "editor.suggestFontSize": 16,
    "editor.suggestSelection": "first",
    "editor.tabSize": 2,
    "editor.wordWrap": "off",
    "emmet.showAbbreviationSuggestions": false,
    "emmet.showExpandedAbbreviation": "never",
    "emmet.showSuggestionsAsSnippets": false,
    "explorer.compactFolders": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.visible": 0,
    "extensions.ignoreRecommendations": true,
    "files.associations": {},
    "files.trimTrailingWhitespace": true,
    "git.enabled": true,
    "github.copilot.chat.agent.thinkingTool": true,
    "github.copilot.enable": {
        "*": false,
        "markdown": false,
        "plaintext": false,
        "scminput": false
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    "html.autoClosingTags": false,
    "html.format.enable": false,
    "http.proxyStrictSSL": false,
    "javascript.format.enable": false,
    "javascript.format.insertSpaceAfterSemicolonInForStatements": false,
    "javascript.preferences.quoteStyle": "single",
    "javascript.suggest.autoImports": false,
    "javascript.updateImportsOnFileMove.enabled": "never",
    "json.format.enable": false,
    "liveshare.featureSet": "insiders",
    "markdown-preview-enhanced.previewTheme": "github-dark.css",
    "markdown.preview.fontSize": 16,
    "notebook.markup.fontSize": 16,
    "notebook.output.fontFamily": "SauceCodePro Nerd Font Mono",
    "notebook.output.fontSize": 16,
    "redhat.telemetry.enabled": false,
    "remote.containers.workspaceMountConsistency": "delegated",
    "scm.inputFontFamily": "SauceCodePro Nerd Font Mono",
    "scm.inputFontSize": 16,
    "scss.validate": true,
    "security.promptForLocalFileProtocolHandling": false,
    "security.workspace.trust.enabled": false,
    "terminal.external.osxExec": "Ghostty.app",
    "terminal.integrated.fontFamily": "SauceCodePro Nerd Font Mono",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.scrollback": 100000,
    "typescript.format.enable": false,
    "typescript.inlayHints.parameterNames.enabled": "none",
    "typescript.preferences.quoteStyle": "single",
    "typescript.suggest.autoImports": false,
    "typescript.updateImportsOnFileMove.enabled": "never",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.newWindowDimensions": "maximized",
    "window.openFilesInNewWindow": "off",
    "window.zoomPerWindow": false,
    "workbench.colorTheme": "Catppuccin Frapp√©",
    "workbench.editor.enablePreview": false,
    "workbench.editor.revealIfOpen": true,
    "workbench.editor.tabActionCloseVisibility": false,
    "workbench.editor.tabSizing": "shrink",
    "workbench.startupEditor": "none",
    "workbench.tree.enableStickyScroll": false,
    "diffEditor.maxComputationTime": 0,
    "workbench.iconTheme": "catppuccin-frappe",
    "chat.mcp.gallery.enabled": true,
    "workbench.activityBar.location": "top",
    "githubPullRequests.notifications": "pullRequests",
    "githubPullRequests.experimental.chat": false,
    "gitlens.ai.model": "vscode",
    "gitlens.ai.vscode.model": "copilot:gpt-4.1",
    "githubPullRequests.fileListLayout": "tree",
    "window.zoomLevel": 1
}